<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Group">
  <test thread-count="1" name="Group Test">
  
  
  <groups>
  <run>
  <include name = "Sanity"></include>
  <exclude name = "Regression"></exclude>
  </run>
  </groups>
  
  
    <classes>
      <class name="Assertion.GroupTest"/>
      <class name="Assertion.HardAssertion"/>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
